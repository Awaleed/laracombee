{
    "name": "amranidev/laracombee",
    "description": "Recommendation system for laravel",
    "keywords": ["recommendation system", "php", "laravel", "recombee", "laravel and recombee", "laravel with recombee"],
    "license": "MIT",
    "type": "laravel-package",
    "support": {
        "issues": "https://github.com/amranidev/laracombee/issues",
        "source": "https://github.com/amranidev/laracombee"
    },
    "require": {
        "recombee/php-api-client": "^2.2",
        "guzzlehttp/promises": "^1.3"
    },
    "require-dev": {
        "phpunit/phpunit": "7.0",
        "orchestra/testbench": "3.7.0"
    },
    "authors": [
        {
            "name": "Houssain Amrani",
            "email": "amranidev@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amranidev\\Laracombee\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amranidev\\Laracombee\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "dev-master"
        },
        "laravel": {
            "providers": [
                "Amranidev\\LAracombee\\Providers\\LaracombeeServiceProvider"
            ]
        }
    }
}
